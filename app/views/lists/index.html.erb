
<div class="cards">

  <% @lists.each do |list| %>
        <%= link_to(list_path(list.id)) do %>
        <div class="card-list" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, crop: :fill, cloud_name: 'dyi5xnjhx' %>')">
          <%= list.name %>
        </div>
       <% end %>
  <% end %>

</div>
