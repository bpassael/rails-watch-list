<h1>New bookmark</h1>
<%= simple_form_for [@list, @bookmark],  data: { turbo: false } do |f| %>
  <%= f.input :comment %>
    <%= f.association :movie, collection: Movie.order('title') %>
  <%= f.submit "Submit bookmark", class: "btn btn-primary" %>
<% end %>



<%= link_to "Back to list", list_path(params['list_id']) %>
